{
  "diagnostic.displayByAle": true,
  "yaml.format.enable": true,
  "python.pythonPath": "python",
  "languageserver": {
    "racket": {
      "command": "racket",
      "args": [
        "--lib",
        "racket-langserver"
      ],
      "filetypes": [
        "scheme",
        "racket",
        "rkt",
        "rktl"
      ]
    }
  }
}
